{
   "name": "Local WAMP 1",
   "desc": "Quick test case set targeting local WAMP servers 'AutobahnPython' and 'WebMQ'.",

   "mode": "fuzzingwampclient",  

   "caseset": "wamp",
   "cases": ["*"],
   "exclude": [],

   "options": {
      "rtt": 0.2,
      "randomize": true,
      "parallel": false
   },

   "testees": [
      {
         "name": "AutobahnPython",
         "desc": "This runs 'testeewampserver' from wstest.",
         "url": "ws://127.0.0.1:9001",
         "exclude": ["3.*"],
         "options":  {
            "rtt": 0.01
         }
      },
      {
         "name": "WebMQ",
         "url": "ws://192.168.1.148/ws",
         "auth": {
            "authKey": null,
            "authSecret": null
         },
         "options": {
            "rtt": 0.02
         }
      }
   ]
}
